        "@types/express": "^5.0.3",
        "@types/node": "^24.2.0",
        "nodemon": "^3.1.10",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "@types/cors": "^2.8.19",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/swagger-ui-express": "^4.1.8",
