        "dotenv": "^17.2.1",
        "express": "^5.1.0",
        "express-validator": "^7.2.1",
        "morgan": "^1.10.1",
        "multer": "^2.0.2",
        "mysql2": "^3.14.3",
        "sequelize": "^6.37.7",
        "sequelize-cli": "^6.6.3",
        "winston": "^3.17.0",
        "@types/uuid": "^10.0.0",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "dayjs": "^1.11.13",
