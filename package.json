{
    "name": "unigom-connect-backend",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "dev": "nodemon src/server.ts",
        "build": "tsc",
        "start": "node dist/server.js",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "migrate": "./node_modules/.bin/sequelize-cli db:migrate",
        "migrate:undo": "./node_modules/.bin/sequelize-cli db:migrate:undo",
        "migrate:undo:all": "./node_modules/.bin/sequelize-cli db:migrate:undo:all",
        "model": "node src/scripts/generate-model.js",
        "migration": "node src/scripts/generate-migration.js",
        "init-db": "ts-node src/scripts/init-db.ts",
        "init": "./node_modules/.bin/sequelize-cli db:migrate:undo:all && node src/scripts/generate-migration.js && init-db"
    },
    "author": "Justin Micah",
    "keywords": [],
    "license": "ISC",
